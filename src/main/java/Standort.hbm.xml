<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>

	<class name="allgemein.model.Standort" table="standort">

		<id name="standort_ID" column="Standort_ID" type="int">
			<generator class="native" />
		</id>

		<property name="name" column="Standort_Name" not-null="true"
			type="string" />

	</class>
</hibernate-mapping>
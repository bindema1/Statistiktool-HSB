<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>

	<class name="benutzungsstatistik.model.Wintikurier" table="wintikurier">

		<id name="wintikurier_ID" column="Wintikurier_ID" type="int">
			<generator class="native" />
		</id>
			
		<property name="anzahl_Gesundheit" column="Anzahl_Gesundheit" not-null="true"
			type="int" />
			
		<property name="anzahl_Technik" column="Anzahl_Technik" not-null="true"
			type="int" />
			
		<property name="anzahl_Wirtschaft" column="Anzahl_Wirtschaft" not-null="true"
			type="int" />
			
		<property name="anzahl_Linguistik" column="Anzahl_Linguistik" not-null="true"
			type="int" />

	</class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>

	<class name="benutzungsstatistik.model.ExterneGruppe" table="externeGruppe">

		<id name="externeGruppe_ID" column="ExterneGruppe_ID" type="int">
			<generator class="native" />
		</id>
		
		<property name="name" column="Name" not-null="true"
			type="string" />
			
		<property name="anzahl_Personen" column="Anzahl_Personen" not-null="true"
			type="int" />
			
		<many-to-one name="benutzungsstatistik" column="Benutzungsstatistik_ID" not-null="true"
			class="benutzungsstatistik.model.Benutzungsstatistik" fetch="select" lazy="false" />

	</class>
</hibernate-mapping>
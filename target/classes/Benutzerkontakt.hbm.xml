<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>

	<class name="benutzungsstatistik.model.Benutzerkontakt" table="benutzerkontakt">

		<id name="benutzerkontakt_ID" column="Benutzerkontakt_ID" type="int">
			<generator class="native" />
		</id>
		
		<property name="timestamp" column="Timestamp" not-null="true"
			type="timestamp" />
			
		<many-to-one name="benutzungsstatistik" column="Benutzungsstatistik_ID" not-null="true"
			class="benutzungsstatistik.model.Benutzungsstatistik" />

	</class>
</hibernate-mapping>